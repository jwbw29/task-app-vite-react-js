{
  "version": 2,
  "builds": [
    {
      "src": "api/tasks.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/main.jsx",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/tasks.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
